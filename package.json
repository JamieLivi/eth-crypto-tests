{
  "name": "eth-crypto-js",
  "version": "0.1.14",
  "description": "Cryptographic javascript-functions for ethereum and tutorials on how to use them together with web3js and solidity",
  "author": "Olivier Esuka <oesukan@gmail.com> (https://oesukam.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oesukam/eth-crypto-js/issues"
  },
  "homepage": "https://github.com/oesukam/eth-crypto-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oesukam/eth-crypto-js.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.esm.js",
    "require": "./dist/index.js"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "blockchain",
    "crypto",
    "eth",
    "ethereum",
    "solidity",
    "evm",
    "identity",
    "key pair",
    "private key",
    "public key",
    "encryption",
    "secp256k1",
    "dapp",
    "ecies",
    "smart-contract",
    "signature"
  ],
  "scripts": {
    "rimraf": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.{ts,js,json}\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run rimraf && microbundle",
    "build:dev": "microbundle watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "audit-ci": "audit-ci --config audit-ci.jsonc",
    "release": "npm publish --access-public"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.5.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "audit-ci": "^7.0.1",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "eccrypto": "^1.1.6",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^6.13.1",
    "secp256k1": "^5.0.0"
  }
}
